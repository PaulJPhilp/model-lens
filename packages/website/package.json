{
	"name": "website",
	"version": "0.1.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bun run src/server.ts",
		"start": "bun run src/server.ts",
		"build": "tsc --noEmit --skipLibCheck",
		"check": "tsc --noEmit",
		"format": "biome format --write .",
		"lint": "biome check --write .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:api": "vitest run tests/api/",
		"test:services": "vitest run tests/services/",
		"test:integration": "vitest run tests/integration/",
		"test:integration:api": "vitest run tests/integration/api/",
		"test:integration:services": "vitest run tests/integration/services/",
		"test:integration:database": "vitest run tests/integration/database/",
		"test:integration:watch": "vitest tests/integration/",
		"db:migrate": "drizzle-kit migrate",
		"db:generate": "drizzle-kit generate",
		"db:push": "drizzle-kit push",
		"sync-models": "bun run src/scripts/sync-models.ts",
		"check-sync-health": "bun run src/scripts/check-sync-health.ts"
	},
	"dependencies": {
		"@effect/platform": "^0.75.0",
		"@effect/platform-bun": "^0.75.0",
		"@effect/experimental": "^0.1.1",
		"@effect/schema": "^0.75.5",
		"@upstash/ratelimit": "^2.0.6",
		"@upstash/redis": "^1.35.4",
		"date-fns": "^3.6.0",
		"drizzle-orm": "^0.44.6",
		"effect": "^3.18.2",
		"pg": "^8.16.3",
		"zod": "^4.1.11"
	},
	"devDependencies": {}
}
